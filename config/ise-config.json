  "sgacls": [
    {
      "name": "Permit-Web-Services",
      "description": "Allow HTTP and HTTPS traffic",
      "acl_content": "permit tcp any any eq 80\npermit tcp any any eq 443\ndeny ip any any"
    },
    {
      "name": "Permit-Voice",
      "description": "Allow voice traffic",
      "acl_content": "permit udp any any range 16384 32767\npermit tcp any any eq 2000\npermit tcp any any eq 5060\ndeny ip any any"
    },
    {
      "name": "Permit-All",
      "description": "Allow all traffic",
      "acl_content": "permit ip any any"
    },
    {
      "name": "Deny-All",
      "description": "Deny all traffic",
      "acl_content": "deny ip any any"
    },
    {
      "name": "Permit-ICMP",
      "description": "Allow ICMP only",
      "acl_content": "permit icmp any any\ndeny ip any any"
    }
  ],
